{
  "name": "monorepo",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "db:start": "docker compose --env-file .env.local up -d",
    "db:stop": "docker compose --env-file .env.local stop",
    "db:logs": "docker compose --env-file .env.local logs -f",
    "dev": "pnpm db:start && dotenv -e .env.local -- turbo run dev --parallel",
    "build": "turbo run build",
    "start": "turbo run start"
  },
  "devDependencies": {
    "dotenv-cli": "11.0.0",
    "prettier": "3.7.4",
    "turbo": "2.6.3"
  }
}
